int	ft_sendout_old(int pid, int bin)
{
	int	i;
	int	n;

	i = 0;
	n = 0;
	while (i < 8)
	{
		if (bin % 10 == 1)
			n += kill(pid, SIGUSR1);
		else
			n += kill(pid, SIGUSR2);
		usleep(10);
		bin /= 10;
		i++;
	}
	return (n);
}